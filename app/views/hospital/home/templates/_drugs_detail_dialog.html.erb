
<el-dialog
  title="药品字典"
  :visible.sync="dialogVisible"
  width="80%"
  >
  <el-row :span="24" :gutter="20">
    <el-col :span="8">
        <el-input
          v-model="searchDrugsList"
          placeholder="请输入药品名/简拼/五笔/编号/"
          style="border: 1px solid #dcdfe6; border-radius: 4px;outline:none"
        >
        </el-input>
    </el-col>
  </el-row>
  <div style="margin-top: 1em;">
    <el-table :data="drugsData">
      <el-table-column prop="name" label="名称"></el-table-column>
      <el-table-column prop="spec" label="规格"></el-table-column>
      <el-table-column prop="name" label="单次数量"></el-table-column>
      <el-table-column prop="name" label="单次剂量"></el-table-column>
      <el-table-column prop="name" label="途径"></el-table-column>
      <el-table-column prop="name" label="频次"></el-table-column>
      <el-table-column prop="name" label="疗程"></el-table-column>
      <el-table-column prop="name" label="总量"></el-table-column>
      <el-table-column prop="name" label="备注"></el-table-column>
    </el-table>
  </div>
  
  <span slot="footer" class="dialog-footer">
    <el-button @click="dialogVisible = false">取 消</el-button>
    <el-button type="primary" @click="dialogVisible = false">确 定</el-button>
  </span>
</el-dialog>