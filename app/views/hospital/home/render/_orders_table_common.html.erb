    <el-table-column
      type="selection"
      width="30">
    </el-table-column>
    <el-table-column
      prop="title"
      min-width="250"
      label="名称">
    </el-table-column>
    <el-table-column
      prop="specification"
      min-width="100"
      align="center"
      label="规格">
    </el-table-column>
    <el-table-column
      min-width="80"
      align="center"
      label="单次数量">
      <template slot-scope="scope">
        <span v-if="scope.row.single_qty">{{scope.row.single_qty.value}}{{scope.row.single_qty.unit}}</span>
      </template>
    </el-table-column>
    <el-table-column
      min-width="80"
      align="center"
      label="单次剂量">
      <template slot-scope="scope">
        <span v-if="scope.row.dose">{{scope.row.dose.value}}{{scope.row.dose.unit}}</span>
      </template>
    </el-table-column>
    <el-table-column
      min-width="50"
      align="center"
      label="途径">
      <template slot-scope="scope">
        <span v-if="scope.row.route">{{scope.row.route.display}}</span>
      </template>
    </el-table-column>
    <el-table-column
      min-width="80"
      align="center"
      label="频次">
      <template slot-scope="scope">
        <span v-if="scope.row.frequency">{{scope.row.frequency.code}}</span>
      </template>
    </el-table-column>
    <el-table-column
      min-width="50"
      align="center"
      label="疗程">
      <template slot-scope="scope">
        <span v-if="scope.row.course_of_treatment">{{scope.row.course_of_treatment.value}}{{scope.row.course_of_treatment.display}}</span>
      </template>
    </el-table-column>
    <el-table-column
      min-width="60"
      align="center"
      label="总量">
      <template slot-scope="scope">
        <span>{{scope.row.total_quantity}}{{scope.row.unit}}</span>
      </template>
    </el-table-column>
    <el-table-column
      min-width="80"
      align="right"
      label="单价">
      <template slot-scope="scope">
        <div align="right">{{scope.row.price | formatMoney}}</div>
      </template>
    </el-table-column>
    <el-table-column
      prop="author.display"
      min-width="80"
      align="center"
      label="医生">
    </el-table-column>
    <el-table-column
      prop="note"
      min-width="100"
      label="备注">
    </el-table-column>
    <el-table-column
      prop="status"
      align="center"
      min-width="80"
      label="状态">
      <template slot-scope="scope">
        <el-tag size="mini" v-if="scope.row.status=='N'">新建</el-tag>
        <el-tag size="mini" type="warning" v-if="scope.row.status=='A'">待缴费</el-tag>
        <el-tag size="mini" type="success" v-if="scope.row.status=='E'">缴费</el-tag>
        <el-tag size="mini" type="info" v-if="scope.row.status=='O'">作废</el-tag>
      </template>
    </el-table-column>