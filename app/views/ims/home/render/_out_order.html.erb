	      @current-change="handleCurrentChange"
	      @selection-change="handleSelectionChange"
	      style="width: 100%">
		    <el-table-column
		      type="selection"
		      width="55">
		    </el-table-column>
	      <el-table-column
	        prop="code"
	        label="编号"
	        width="180">
	      </el-table-column>
	      <el-table-column
	        prop="name"
	        label="姓名"
	        width="180">
	      </el-table-column>
	      <el-table-column
	        prop="amount"
	        label="金额">
	      </el-table-column>
	    </el-table>
			  <div slot="footer" class="dialog-footer">
