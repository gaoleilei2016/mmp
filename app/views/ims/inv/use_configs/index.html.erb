<p id="notice"><%= notice %></p>

<h1>Ims Inv Use Configs</h1>

<table>
  <thead>
    <tr>
      <th>Use system</th>
      <th>Config</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ims_inv_use_configs.each do |ims_inv_use_config| %>
      <tr>
        <td><%= ims_inv_use_config.use_system %></td>
