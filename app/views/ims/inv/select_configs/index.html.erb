<p id="notice"><%= notice %></p>

<h1>Ims Inv Select Configs</h1>

<table>
  <thead>
    <tr>
      <th>Use org</th>
      <th>C</th>
      <th>Use name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ims_inv_select_configs.each do |ims_inv_select_config| %>
      <tr>
        <td><%= ims_inv_select_config.use_org_id %></td>
        <td><%= ims_inv_select_config.c_id %></td>
        <td><%= ims_inv_select_config.use_name %></td>
        <td><%= link_to 'Show', ims_inv_select_config %></td>
        <td><%= link_to 'Edit', edit_ims_inv_select_config_path(ims_inv_select_config) %></td>
        <td><%= link_to 'Destroy', ims_inv_select_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ims Inv Select Config', new_ims_inv_select_config_path %>
